#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/plataforma-enem/node_modules/.pnpm/superstatic@10.0.0_encoding@0.1.13/node_modules/superstatic/lib/bin/node_modules:/home/ubuntu/plataforma-enem/node_modules/.pnpm/superstatic@10.0.0_encoding@0.1.13/node_modules/superstatic/lib/node_modules:/home/ubuntu/plataforma-enem/node_modules/.pnpm/superstatic@10.0.0_encoding@0.1.13/node_modules/superstatic/node_modules:/home/ubuntu/plataforma-enem/node_modules/.pnpm/superstatic@10.0.0_encoding@0.1.13/node_modules:/home/ubuntu/plataforma-enem/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/plataforma-enem/node_modules/.pnpm/superstatic@10.0.0_encoding@0.1.13/node_modules/superstatic/lib/bin/node_modules:/home/ubuntu/plataforma-enem/node_modules/.pnpm/superstatic@10.0.0_encoding@0.1.13/node_modules/superstatic/lib/node_modules:/home/ubuntu/plataforma-enem/node_modules/.pnpm/superstatic@10.0.0_encoding@0.1.13/node_modules/superstatic/node_modules:/home/ubuntu/plataforma-enem/node_modules/.pnpm/superstatic@10.0.0_encoding@0.1.13/node_modules:/home/ubuntu/plataforma-enem/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/bin/server.js" "$@"
else
  exec node  "$basedir/../../lib/bin/server.js" "$@"
fi
